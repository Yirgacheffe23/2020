#include <stdio.h>
//判断奇偶
int main()
{
	int num = 0;
	scanf_s("%d", &num);
	//if (num = 0);
	//{
	//	printf("为偶数", num);
	//}
	if (num % 2 == 0)
	{
		printf("为偶数", num);
	}
	else
	{
		printf("为奇数", num);
	}
	return 0;
}


